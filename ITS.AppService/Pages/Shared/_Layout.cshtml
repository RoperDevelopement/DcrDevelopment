                            <ul class="dropdown-menu" role="menu">
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-page="/EdocsITSUsers/EdocsITSUsersView" onmouseover="$(this).css('background','yellow');" onmouseout="$(this).css('background','white');">User Profiles</a>
                                </li>



