        <li class="nav-item dropdown">
            <a class="nav-link text-dark dropdown-toggle" data-toggle="dropdown" role="link" asp-area="" asp-page="" onmouseover="$(this).css('background','#7abdff');" onmouseout="$(this).css('background','white');">Lookup Records</a>
            <ul class="dropdown-menu" role="menu">
