                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-page="/TrackingSystemReports/ITSReportByTrackingIDView" onmouseover="$(this).css('background','yellow');" onmouseout="$(this).css('background','white');">TrackinID</a>
                                </li>

                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-page="/TrackingSystemReports/ITSReportsView" onmouseover="$(this).css('background','yellow');" onmouseout="$(this).css('background','white');">Inventory Reports</a>
                                </li>

                                @*
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-page="/TrackingSystemReports/ITSReportCostView" onmouseover="$(this).css('background','yellow');" onmouseout="$(this).css('background','white');">Cost Report</a>
