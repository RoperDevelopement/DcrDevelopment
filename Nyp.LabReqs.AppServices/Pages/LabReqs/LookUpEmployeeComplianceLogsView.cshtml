@if ((@Model.IDicEmployeeComplianceLogsModel != null))
{
    <div id="divLabReqs">
        <p class="text-left">Total Query Time @Model.TotalQueryTime</p>
        <p class="p-0" />
        <table class="table table-hover w-auto" width="100%" id="dtLabReqs">
            <thead>
                <tr>
                    <th>File URL</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>ID Number</th>
                    <th>Department</th>
                    <th>Document Type</th>
                    <th>Job Title</th>
                    <th>Scan Operator</th>
                    <th>Scan Date</th>
                    <th>Scan Batch ID</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var items in Model.IDicEmployeeComplianceLogsModel)
