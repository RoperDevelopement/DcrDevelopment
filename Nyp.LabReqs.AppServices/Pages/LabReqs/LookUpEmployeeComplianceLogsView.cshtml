@section scripts{
    <script>
        $(document).ready(function () {
            SessionExpireAlert(0);
            $(this).bind("contextmenu", function (e) {

                e.preventDefault();

            });
            //from year
            if ($('#searchStartDate').val() != '') {
                $('#checkboxSearchDates').css("display", "block");
                $("#searchBydate").val('searchByLogDate');
                $('#lDate').prop('checked', true);
                $('#sDate').prop('checked', false);
                $('#btnlookUpByDate').css("display", "block");
            }
            CompleteAjaxCall();

            $("#divLoading").hide();


            //search from year
            $('#btnFirstLastName').click(function () {

                $("#divMain").fadeOut(100);
                $('#myModalFirstLastName').css("display", "block");
            });
            $('#btnCloseFirstLastName').click(function () {
                $("#divMain").fadeIn(100);
                $('#myModalFirstLastName').css("display", "none");
                $('#searchempFLNameBlank').css("display", "none");
            });
            //emp first last name
            $('#btnsearchFirstLastName').click(function () {
                var empFName = document.querySelectorAll("[id='ComplianceLogsModel_FirstName']");
                var empLName = document.querySelectorAll("[id='ComplianceLogsModel_LastName']");
                if (CheckEmptyStr(empFName[0].value, empLName[0].value)) {

                    ShowHideDialog("searchempFLNameBlank");

                    return false;
                }
                var message = "Getting Comliance Logs for Employee First Name " + empFName[0].value + ' Employee Last Name ' + empLName[0].value;

                ShowLoadingDialog(message);
                $('#btnCloseFirstLastName').click();
                $("<input type='hidden' value='pFName' />")
                    .attr("id", "searchby")
                    .attr("name", "searchby")
                    .prependTo("#searchFirstLastName");
                $("<input type='hidden'/>")
                    .attr("value", $('#searchStartDate').val())
                    .attr("id", "startSDate")
                    .attr("name", "startSDate")
                    .prependTo("#searchFirstLastName");
                $("<input type='hidden'/>")
                    .attr("value", $('#searchEndDate').val())
                    .attr("id", "endSDate")
                    .attr("name", "endSDate")
                    .prependTo("#searchFirstLastName");
                $("#searchFirstLastName").submit();

            });
            //scan operator
            $('#btnScanOpt').click(function () {
                //  SetValueUncehckCheckBoxValues();
                $("#divMain").fadeOut(100);
                $('#myModalscanOp').css("display", "block");
            });
            $('#btnScanOperClose').click(function () {
                $("#divMain").fadeIn(100);
                $('#myModalscanOp').css("display", "none");
            });

            $(document).keypress(function (e) {
                if (e.which == 13) {

                    if ($('#myModalFirstLastName').is(':visible')) {

                        $('#btnsearchFirstLastName').click();
                        return false;

                    }
                    else if ($('#myModalscanOp').is(':visible')) {

                        $('#btnsearchscanOp').click();
                        return false;

                    }
                    else if ($('#myModalEmpID').is(':visible')) {

                        $('#btnsearchIDEmp').click();
                        return false;

                    }
                    else if ($('#myModalEmpDep').is(':visible')) {

                        $('#btnsearchEmpDep').click();
                        return false;

                    }
                    else if ($('#myModalEmpJobTitle').is(':visible')) {

                        $('#btnsearchEmpJobTitle').click();
                        return false;

                    }
                    else if ($('#myModalEmpDocType').is(':visible')) {

                        $('#btnsearchEmpDocType').click();
                        return false;

                    }
                    else {
                        return true;
                    }
                }
                return true;
            });




            $('#btnsearchscanOp').click(function () {
                if (CheckEmptyStr($('#ComplianceLogsModel_ScanOperator').val())) {

                    ShowHideDialog("searchScanOprBlank");

                    return false;
                }

                $('#btnScanOperClose').click();
                var message = "Looking up LookUp Employee Compliance Logs for Scan Operator " + $('#ComplianceLogsModel_ScanOperator').val();
                ShowLoadingDialog(message);

                $("<input type='hidden' value='searchbyScanOperator' />")
                    .attr("id", "searchby")
                    .attr("name", "searchby")
                    .prependTo("#searchscanOp");
                $("<input type='hidden'/>")
                    .attr("value", $('#searchStartDate').val())
                    .attr("id", "startSDate")
                    .attr("name", "startSDate")
                    .prependTo("#searchscanOp");
                $("<input type='hidden'/>")
                    .attr("value", $('#searchEndDate').val())
                    .attr("id", "endSDate")
                    .attr("name", "endSDate")
                    .prependTo("#searchscanOp");
                $("#searchscanOp").submit();

            });

            //employee id
            $('#btnsearchIDEmp').click(function () {

                if ($('#selEmpID').val() == undefined) {
                    ShowHideDialog("searchempIDBlank");
                    return false;
                }

                if ($('#selEmpID').val() == 'string-empty') {

                    ShowHideDialog("searchempIDBlank");
                    return false;
                }

                var message = "Getting Compliance Logs by Employee ID " + $('#selEmpID').val();
                ShowLoadingDialog(message);
                $('btnCloseEmpID').click();
                $("<input type='hidden'/>")
                    .attr("value", $('#searchStartDate').val())
                    .attr("id", "startSDate")
                    .attr("name", "startSDate")
                    .prependTo("#searchEmpID");
                $("<input type='hidden'/>")
                    .attr("value", $('#searchEndDate').val())
                    .attr("id", "endSDate")
                    .attr("name", "endSDate")
                    .prependTo("#searchEmpID");
                $("<input type='hidden' value='searchbyEmpID' />")
                    .attr("id", "searchby")
                    .attr("name", "searchby")
                    .prependTo("#searchEmpID");
                $("<input type='hidden'/>")
                    .attr("value", $('#selEmpID').val())
                    .attr("id", "perfLabCode")
                    .attr("name", "perfLabCode")
                    .prependTo("#searchEmpID");
                $("#searchEmpID").submit();

            });

            //emp dep
            $('#btnEmpDept').click(function () {
                $("#divMain").fadeOut(100);
                $('#myModalEmpDep').css("display", "block");
            });
            $('#btnCloseEmpDep').click(function () {
                $("#divMain").fadeIn(100);
                $('#myModalEmpDep').css("display", "none");
                $('#searchempDepBlank').css("display", "none");
            });

            //emp doc type
            $('#btnEmpDocType').click(function () {
                $("#divMain").fadeOut(100);
                $('#myModalEmpDocType').css("display", "block");
            });
            $('#btnCloseEmpDocType').click(function () {
                $("#divMain").fadeIn(100);
                $('#myModalEmpDocType').css("display", "none");
                $('#searchempDocTypeBlank').css("display", "none");
            });

            //emp jobtitile
            $('#btnEmpJobTitle').click(function () {
                $("#divMain").fadeOut(100);
                $('#myModalEmpJobTitle').css("display", "block");
            });
            $('#btnCloseEmpJobTitle').click(function () {
                $("#divMain").fadeIn(100);
                $('#myModalEmpJobTitle').css("display", "none");
                $('#searchempJobTitleBlank').css("display", "none");
