<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BMRMobileApp.SearchWebPage"
             Title="Search">
    <VerticalStackLayout>
        <Entry x:Name="SearchEntry" Placeholder="Enter search term" Margin="20,40,20,10"/>
        <Button Text="Search" Clicked="OnSearchButtonClicked" BackgroundColor="LightBlue" TextColor="White"/>
    </VerticalStackLayout>
</ContentPage>